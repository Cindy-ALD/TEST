<script>
$(document).ready(function(){
	// standard on load code goes here with $ prefix
	// note: the $ is setup inside the anonymous function of the ready command
	
	/* -------------------------------------------- */
	/* ------------- GLOBAL VARIABLES ------------- */
	/* -------------------------------------------- */
	
	/* jQuery instances */
	
	var $priceHtml = $('.cars-price');
	
	$(".range-km").append("<div class='disable-option'></div>");
$(".range-duree").append("<div class='disable-option'></div>");
$(".loyer-checked").css({"display":"none"});

/* Change select toggle Range inputs */
$(document).on("change", "#cars", function(e) {
  if ($(this).children("option:first-child").is(":selected")) {
    $(".range-km").find(".disable-option").css({"display":"block"});
    $(".range-duree").find(".disable-option").css({"display":"block"});
    $(".range-km, .range-duree").find(".disable-option").attr("title", "Sélectionnez un véhicule pour activer cette option");
  } else {
    $(".range-km").find(".disable-option").css({"display":"none"});
    $(".range-duree").find(".disable-option").css({"display":"none"});
    $(".range-km, .range-duree").find(".disable-option").attr("title", "");
    if ($(this).children("option[value='BMW Série 1']").is(":selected")) {
      $("label[for=checkboxLoyer]").text("Premier loyer majoré de 2 691€"); // BMW Série 1
    } else if($(this).children("option[value='BMW iX1']").is(":selected")){
      $("label[for=checkboxLoyer]").text("Premier loyer majoré de 5 059€"); // BMW iX1
    } else if($(this).children("option[value='BMW X3']").is(":selected")){
      $("label[for=checkboxLoyer]").text("Premier loyer majoré de 5 350€"); // BMW X3
    } else if($(this).children("option[value='Autre']").is(":selected")){
      $("label[for=checkboxLoyer]").text("Intéressé(e) par un premier loyer majoré ?"); // Autre
    } else {
      $("label[for=checkboxLoyer]").text("Premier loyer majoré default"); // Default
    }
  }
});

/* Show/hide Loyer input */
$(".loyer-checkbox").on("change", function(e) {
  if ($(this).is(":checked")) {
    $(".loyer-checked").css({"display":"block"});
  } else {
    $(".loyer-checked").css({"display":"none"});
  }
});

/* Partie 2 */
$(".range-km input, .range-duree input").hover(function() {
  // Vérifier si la partie 1 a été sélectionnée
  if ($("#cars").children("option:first-child").is(":selected")) {
    // Afficher le message d'erreur
    $(this).attr("title", "Sélectionnez un véhicule pour activer cette option");
  } else {
    // Masquer le message d'erreur
    $(this).attr("title", "");
  }
});

