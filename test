/* -------------------------------------------- */

// Document is ready
$(document).ready(function () {
  $priceHtml.html(formatPriceString(defaultPrice));
});

// Document has loaded
$(window).on('load', function () {
  function initializeJauges() {
	$('.jauge').each(function () {
	  var $this = $(this);

	  var firstDot = $this.find('.line-option_selector')[0];
	  var distanceLeft = $(firstDot).offset().left - $this.offset().left;

	  $(this).find('.line-active').css('padding-left', distanceLeft);
	});
  }

  initializeJauges();
});

/* -------------------------------------------- */
/* --------------- LISTENERS ------------------ */
/* -------------------------------------------- */

/* Main listener */

function onCarFieldChange() {
  // Car model value
  var $carModel = $('#cars').val();
  // Kilometers value
  var $kilometers = $('.range-km .line-option_selector.option-active').attr(
	'value'
  );
  // Duration value
  var $duration = $('.range-duree .line-option_selector.option-active').attr(
	'value'
  );

  // Checkbox value
  var $loan = $('#checkboxLoyer').is(':checked') ? 'loyerOn' : 'loyerOff';

  // Get unique key by separating each value with a '_'
  var key = [$carModel, $duration, $kilometers, $loan].join('_');

  // Map this value to cars variable, if not found use defaultPrice
  var startingPrice = cars[key] || defaultPrice;

  // Write startingPrice in html
  $priceHtml.html(formatPriceString(startingPrice));
}

/* Jauge listener */

var kmActive = 0;
var durationActive = 0;

function onJaugeChange(index, container, active) {
  var $container = container;
  var $lineContainer = $($container.find('.line-active'));
  var $line = $($container.find('.line-active_line'));

  var $dot = $(
	$($container.find('.line-option')[index - 1]).find('.line-option_selector')
  );

  // Left margin to horizontally align active dot
  var leftSpacing =
	parseInt($container.find('.line-active').css('padding-left'), 10) - 1;

  // If jauge is going forward
  if (index > active) {
	for (let i = active; i < index; i++) {
	  setTimeout(
		function () {
		  $($container.find('.line-option')[i])
			.find('.line-option_selector')
			.addClass('option-completed');
		},
		// We need an exception when we animate the first dot so that's updated instantly to avoid any flicker
		i === active ? 0 : 400 / (index - i)
	  );
	}
  } else {
	// If jauge is going backward
	for (let i = active; i > index - 1; i--) {
	  setTimeout(function () {
		$($container.find('.line-option')[i])
		  .find('.line-option_selector')
		  .removeClass('option-completed');
	  }, i === index - 1 ? 4000 : 400 / (index + i));
	}
  }

  $lineContainer.css('display', 'flex');

  $line.animate({
	duration: 400,
	easing: 'easeout',
	width: $dot.position().left - leftSpacing,
  });

  return index;
}

// Window resize
$(window).resize(function () {
  // Function used when window is resized - to calculate jauge style values
  function calculateJauge(container, active) {
	var $container = container;
	var $line = $($container.find('.line-active_line'));

	var $dot = $(
	  $($container.find('.line-option')[active - 1]).find(
		'.line-option_selector'
	  )
	);

	var leftSpacing =
	  parseInt($container.find('.line-active').css('padding-left'), 10) - 1;

	$line.css('width', $dot.position().left - leftSpacing);
  }

  // KMs
  calculateJauge($('.range-km'), kmActive);
  // Duration
  calculateJauge($('.range-duree'), durationActive);
});

/* On car model change */

$('.cars-select').on('change', function () {
  if ($(this).val() == 'Skoda Fabia') {
	$('.modeleA').css('display', 'flex');
	$('.modeleB').css('display', 'none');
	$('.modeleC').css('display', 'none');
	$('.modeleD').css('display', 'none');
  
  } else if ($(this).val() == 'Skoda Kamiq') {
	$('.modeleA').css('display', 'none');
	$('.modeleB').css('display', 'flex');
	$('.modeleC').css('display', 'none');
	$('.modeleD').css('display', 'none');

  } else if ($(this).val() == 'Skoda Enyaq') {
	$('.modeleA').css('display', 'none');
	$('.modeleB').css('display', 'none');
	$('.modeleC').css('display', 'flex');
	$('.modeleD').css('display', 'none');
   
	} else if ($(this).val() == 'Autre') {
	$('.modeleA').css('display', 'none');
	$('.modeleB').css('display', 'none');
	$('.modeleC').css('display', 'none');
	$('.modeleD').css('display', 'flex');
  }  

  // Trigger listener
  onCarFieldChange();
});

/* On kms change */

$('.range-km').on('click', '.line-option', function () {
  var $selector = $($(this)).find('.line-option_selector');
  var $previousActive = $('.range-km .line-option_selector.option-active');
  var index = $(this).index();

  // Remove previous
  $previousActive.removeClass('option-active');
  $($previousActive.siblings('.label-active')).removeClass('label-active');
  $($previousActive.siblings('.line-option_label')).removeClass('hide');

  // Add pressed state to new one
  $selector.addClass('option-active');
  $($selector.siblings('.line-option_sublabel')).addClass('label-active');
  $($selector.siblings('.line-option_label')).addClass('hide');

  // Trigger listener
  onCarFieldChange();

  // Jauge functions
  var newIndex = onJaugeChange(index, $('.range-km'), kmActive);
  kmActive = newIndex;
});

/* On duration change */

$('.range-duree').on('click', '.line-option', function () {
  var $selector = $($(this)).find('.line-option_selector');
  var $previousActive = $('.range-duree .line-option_selector.option-active');
  var index = $(this).index();

  // Remove previous
  $previousActive.removeClass('option-active');
  $($previousActive.siblings('.label-active')).removeClass('label-active');
  $($previousActive.siblings('.line-option_label')).removeClass('hide');

  // Add pressed state to new one
  $selector.addClass('option-active');
  $($selector.siblings('.line-option_sublabel')).addClass('label-active');
  $($selector.siblings('.line-option_label')).addClass('hide');

  // Trigger listener
  onCarFieldChange();

  // Jauge functions
  var newIndex = onJaugeChange(index, $('.range-duree'), durationActive);
  durationActive = newIndex;
});

/* On checkbox change */

$('#checkboxLoyer').on('change', onCarFieldChange);

/* On quote button click */
var $alreadyAppendKm = false;
var $alreadyAppendDuree = false;

$('.quote-button').click(function (e) {
  var $error =
	'<span class="wrapper-error"> - Merci de renseigner ce champ </span>';

  if (!$('.range-km .line-option_selector').hasClass('option-active')) {
	if ($alreadyAppendKm == false) {
	  $('.range-km h4').append($error);
	  $alreadyAppendKm = true;
	}
	var $counterError = 1;
  } else {
	var $kmVal = $('.range-km .line-option_selector.option-active').attr(
	  'value'
	);
	$('#fe10163').val($kmVal);
  }

  if (!$('.range-duree .line-option_selector').hasClass('option-active')) {
	if ($alreadyAppendDuree == false) {
	  $('.range-duree h4').append($error);
	  $alreadyAppendDuree = true;
	}
	var $counterError = 1;
  } else {
	var $dureeVal = $('.range-duree .line-option_selector.option-active').attr(
	  'value'
	);
	$('#fe10164').val($dureeVal);
  }

  var $carsModele = $('.cars-select').val();
  $('#fe10166').val($carsModele);

  if ($('#checkboxLoyer').is(':checked')) {
	var $loyer = 'Oui';
	$('#fe10165').val($loyer);
  } else {
	var $loyer = 'Non';
	$('#fe10165').val($loyer);
  }

  console.log($loyer);
  console.log($dureeVal);
  console.log($kmVal);
  console.log($carsModele);
  // Display form 2
  if ($counterError !== 1) {
	$('.form-step1').toggle();
	$('.elq-form').toggle();
  }
});

/* On previous button click */

$('.previous-step').click(function () {
  $('.form-step1').toggle();
  $('.elq-form').toggle();
});

  
  });
</script>
